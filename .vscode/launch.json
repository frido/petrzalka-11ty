    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\generators\\opendata.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "autoAttachChildProcesses": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch 11ty",
            "program": "${workspaceFolder}\\node_modules\\@11ty\\eleventy\\cmd.js",
            "args": [
                // Just an example argument
                // "--output=C:\\Temp"
            ],
        }
    ]
}