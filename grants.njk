---
layout: layouts/home.njk
eleventyNavigation:
  key: Granty
  order: 3
---
<div class="paletteC">
  <section class="container">
    <header>
      <h1 class="container-title">Športové granty</h1>
    </header>
    <div class="flex-wrap c-4">
      {% for grantPost in collections.sgrant | head(1)  %}
        {% set grants = grantPost.data.list %}
        {% for grant in grants  %}
          {% include "grantslist.njk" %}
        {% endfor %}
      {% endfor %}
    </div>
  </section>
</div>

<div class="paletteC">
  <section class="container">
    <header>
      <h1 class="container-title">Dotácie</h1>
    </header>
    <div class="flex-wrap c-4">
      {% for grantPost in collections.ogrant | head(1)  %}
        {% set grants = grantPost.data.list %}
        {% for grant in grants  %}
          {% include "grantslist.njk" %}
        {% endfor %}
      {% endfor %}
    </div>
  </section>
</div>